    "@swc/core": "^1.2.173",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "16.11.7",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "eslint-config-xo-nextjs": "^4.1.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "nx": "14.2.4",
    "prettier": "^2.6.2",
    "tslib": "^2.4.0",
    "type-fest": "^2.13.0",

    "next": "^12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "regenerator-runtime": "0.13.7",
    "tslib": "^2.0.0"
  },
  "xo": {
    "extends": "xo-nextjs",
    "plugins": [
      "simple-import-sort"
    ],
    "prettier": true,
    "rules": {
      "simple-import-sort/imports": "error",
      "simple-import-sort/exports": "error",
      "@typescript-eslint/consistent-type-imports": [
        "warn"
      ]
    }
  },
  "prettier": {
    "semi": true,
    "useTabs": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "singleQuote": true,
    "bracketSpacing": true
